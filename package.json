{
  "name": "node-foundation",
  "version": "0.0.1",
  "author": "Real_Marshal",
  "main": "src/index.ts",
  "packageManager": "yarn@3.3.0",
  "private": true,
  "scripts": {
    "docker": "docker compose up -d --no-recreate",
    "dev": "yarn docker && cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --transpile-only --cache-directory .cache/ts-node-dev ./src/index.ts",
    "build": "tsc",
    "test:prepare": "docker compose up -d --no-recreate postgres_test",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "start": "cross-env NODE_ENV=production TS_NODE_BASEURL=./dist/src yarn node -r tsconfig-paths/register dist/src/index.js",
    "postinstall": "husky install",
    "lint": "eslint . --cache --cache-location .cache/eslint --cache-strategy content",
    "lint:nocache": "eslint .",
    "format": "prettier --check ./src",
    "commit": "gacp --no-add --emoji emoji",
    "generate:schemas": "kanel && prettier -w ./src/schemas",
    "migrate:make": "yarn knex migrate:make",
    "migrate:list": "yarn knex migrate:list",
    "migrate:up": "cross-env NODE_OPTIONS='-r ts-node/register' knex migrate:up && schemalint & yarn generate:schemas",
    "migrate:down": "cross-env NODE_OPTIONS='-r ts-node/register' knex migrate:down"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.13",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "gacp": "^3.0.3",
    "husky": "^8.0.2",
    "hygen": "^6.2.11",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "kanel": "^3.1.3",
    "knex": "^2.3.0",
    "pg": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "schemalint": "^0.5.10",
    "semantic-release": "^19.0.5",
    "semantic-release-docker": "^2.2.0",
    "testcontainers": "^9.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/sensible": "^5.2.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@sinclair/typebox": "^0.25.13",
    "ajv": "^8.11.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.4.0",
    "lodash": "^4.17.21",
    "postgres": "^3.3.2",
    "tsconfig-paths": "^4.1.1"
  }
}
